{
  "name": "github-backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "test": "jest",
    "dev": "nodemon --watch src/server.ts",
    "studio": "npx prisma studio --port 5555 --browser none",
    "build": "tsc",
    "migrate": "npx prisma generate && npx prisma migrate deploy",
    "docker:dev": "npm run build && docker-compose up --build",
    "docker:prod": "npm run build && docker-compose -f docker-compose.prod.yml up --build",
    "docker:prod:stop": "docker-compose -f docker-compose.prod.yml down",
    "frontend": "docker-compose -f docker-compose.yml up -d --build frontend",
    "frontend:stop": "docker-compose -f docker-compose.yml down",
    "start": "node dist/server.js",
    "start:docker": "docker exec -it app npm start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.17.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "nodemon": "^3.1.11",
    "serverless-http": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/supertest": "^6.0.3",
    "concurrently": "^9.0.1",
    "jest": "^30.2.0",
    "jest-mock-extended": "^4.0.0",
    "prisma": "^6.17.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
