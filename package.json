{
  "name": "github-backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:docker": "docker compose up -d app && docker compose up -d db",
    "studio": "prisma studio --port 5555 --browser none",
    "dev:studio": "concurrently -k -n api,studio -c green,blue \"npm run dev\" \"npm run studio\"",
    "build": "tsc",
    "prestart": "prisma generate",
    "start": "node dist/server.js",
    "start:docker": "docker exec -it app npm start",
    "postinstall": "prisma generate",
    "prisma:generate": "docker exec -it db prisma generate",
    "prisma:migrate:dev": "docker exec -it db prisma migrate dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.17.0",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/node": "^24.7.1",
    "concurrently": "^9.0.1",
    "prisma": "^6.17.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}